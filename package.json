    "good": "^8.1.0",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^17.2.2",
    "hapi-auth-jwt2": "^8.0.0",
    "hapi-routes": "^5.0.0",
    "immutable": "^3.8.2",
    "inert": "^5.1.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "mongoose": "^5.0.10",
    "muicss": "^0.9.36",
    "node-env-file": "^0.1.8",
    "nodemailer": "^4.6.2",

  "author": "Paul Reesman <paul.r.reesman@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --progress --colors --display-error-details",
    "dev": "npm run dev:build && npm run start",
    "dev:build": "npm run build -- --mode development",

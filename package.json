    "eslint:fix": "eslint **/*.js --fix",
    "prod": "NODE_ENV=production npm run prod:build && npm run start",
    "prod:build": "npm run build -- --mode production",
    "start": "nodemon server.js",
    "test": "nyc mocha --reporter spec"
  },
  "exclude": [
    "**/test/**/*.js"
  ],
  "dependencies": {
